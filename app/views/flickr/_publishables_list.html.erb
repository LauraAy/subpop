<% publishables.each do |item| -%>
<div class="row">
<div class="publishable-status<%= item.processing? ? ' processing' : '' -%>" id="publishable-<%= item.model_name.element %>-<%= item.id -%>" data-item="<%= item.id -%>" data-item-type="<%= item.class.name.underscore -%>">
    <%= render '/flickr/status', item: item -%>
  </div>
</div>
<% end -%>